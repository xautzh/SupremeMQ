<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
		http://www.springframework.org/schema/context
    	http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:component-scan base-package="com.xaut.server.dispatch"/>
    <context:component-scan base-package="com.xaut.server.manager"/>
    <context:component-scan base-package="com.xaut.server.transport"/>
    <context:component-scan base-package="com.xaut.server.queue"/>

    <bean id="placeholderConfig"
          class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
        <property name="locations">
            <list>
                <value>classpath:supremeMq-config.properties</value>
            </list>
        </property>
    </bean>
</beans>